<nav>
    <a href="/">Return Home</a>
</nav>
<form>
    <div class="form-group">
        <h1 id="benchTitle">BENCH PRESS!</h1>
        <h2>Please enter your bench presses data:</h2>
        <label for="weight">Enter weight (kg):</label>
            <input class="form-control" type="number" id="weight" name="weight" min="0" max="1000">
        <label for="repetitions">Enter repetitions:</label>
            <input class="form-control" type="number" id="repetitions" name="repetitions" min="0" max="1000">
        
        <button type="button" class="btn btn-primary submit" id="submitBench">Submit Bench Presses</button>

    </div>
</form>

<table class="table table-dark" id="runTable">
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">Weight(kg)</th>
      <th scope="col">Repetitions</th>
      <th scope="col">Bench Press Date</th>
    </tr>
  </thead>
  <tbody>
      {{#each pbench}}
    <tr>
      <th scope="row">{{@index}}</th>
      <td>{{this.weight}}</td>
      <td>{{this.repetitions}}</td>
      <td>{{this.created}}</td>
    </tr>
     {{/each}}
  </tbody>
</table>

<script src="./js/app.js" type="text/javascript"></script>
